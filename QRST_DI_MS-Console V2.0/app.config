<?xml version="1.0"?>
<configuration>
  <configSections>
  </configSections>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
  <appSettings>
    <!--通信服务器连接字符串-->
    <add key="DbServerIp" value="10.10.10.25" />
    <add key="BaseUtilityTcpPort" value="51112" />
    <!--系统语言设置 中文=ch，英文=en，默认中文-->
    <add key="SystemLanguage" value="en" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />

    <!--数据库通信方式.single/multiple. SQLite版可single/multiple. MySQL版只multiple-->
    <add key="DbCommunication" value="multiple"/>

    <!-- 共享目录IP-->
    <add key="SubTaskDataPath" value="10.10.10.25"/>

    <add key="NameNodeIP" value="10.10.10.30"/>
    <add key="NameNodeName" value="namenode"/>
    <add key="DataNodeName" value="datanode"/>
    <add key="NameNodeUsr" value="root"/>
    <add key="NameNodePwd" value="checc"/>
    <add key="HadoopHome" value="/usr/local/hadoop"/>
    <add key="MountPath" value="/root/share"/>

    <!--单时相全覆盖获取瓦片拇指图-->
     <add key="QRST_DI_MS_Console_WS_QDB_GetDataService_GetTileImgView" value="http://202.121.180.89/WS_QDB_GetData/GetTileTinyView.aspx" /> 
  </appSettings>
</configuration>