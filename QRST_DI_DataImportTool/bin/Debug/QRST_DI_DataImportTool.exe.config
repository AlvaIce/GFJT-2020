<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  </configSections>
  <log4net>
    <root>
      <level value="ALL" />
      <appender-ref ref="rollingFile" />
    </root>

    <appender  name="rollingFile" type="log4net.Appender.RollingFileAppender,log4net" >
      <param name="File" value="log.txt" />
      <param name="AppendToFile" value="true" />
      <param name="RollingStyle" value="Date" />
      <param name="DatePattern" value="yyyy.MM.dd" />
      <param name="StaticLogFileName" value="true" />
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%d [%t] %-5p %c - %m%n" />
        <param name="Header" value="&#13;&#10;----------------------header--------------------------&#13;&#10;" />
        <param name="Footer" value="&#13;&#10;----------------------footer--------------------------&#13;&#10;" />
      </layout>
    </appender>
    <appender name="consoleApp" type="log4net.Appender.ConsoleAppender,log4net">
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%d [%t] %-5p %c - %m%n" />
      </layout>
    </appender>
    <logger name="Log4NetTest.LogTest">
      <level value="All" />
      <appender-ref  ref="rollingFile" />
    </logger>
  </log4net>
  
  <appSettings>
    <add key="ConnectionStringMySql" value="server =192.168.10.109; user id = HJDATABASE_ADMIN; password = dbadmin_2011; charset = utf8; database = midb"/>
    <add key="FileServerIP" value="192.168.10.109"/>
    <add key="INDUSTRYCODE" value="0001"/>
    <add key="VectorGroupCode" value="BSDB-2-48"/>

    <!--通信服务器连接字符串-->
    <add key="DbServerIp" value="10.10.10.25" />
    <add key="BaseUtilityTcpPort" value="51112" />
    <!--数据库通信方式.single/multiple. SQLite版可single/multiple. MySQL版只multiple-->
    <add key="DbCommunication" value="multiple"/>
  </appSettings>
  <startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
