        </s:sequence>
      </s:complexType>
      <s:simpleType name="FieldOperator">
        <s:restriction base="s:string">
          <s:enumeration value="Eto" />
          <s:enumeration value="NEto" />
          <s:enumeration value="Gto" />
          <s:enumeration value="GEto" />
          <s:enumeration value="Lto" />
          <s:enumeration value="LEto" />
          <s:enumeration value="Liketo" />
          <s:enumeration value="NLiketo" />
          <s:enumeration value="NotDefine" />
