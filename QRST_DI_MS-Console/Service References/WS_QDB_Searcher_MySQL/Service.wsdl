        </s:sequence>
      </s:complexType>
      <s:simpleType name="LogicalOperator">
        <s:restriction base="s:string">
          <s:enumeration value="ANDto" />
          <s:enumeration value="ORto" />
          <s:enumeration value="NotDefine" />
        </s:restriction>
      </s:simpleType>
      <s:complexType name="ArrayOfSimpleCondition">
